{
  "name": "ai-suite-server",
  "version": "1.0.0",
  "description": "Backend API for AI Productivity Suite",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:generate": "npx prisma generate"
  },
  "dependencies": {
    "express": "4.18.0",
    "cors": "2.8.0",
    "helmet": "7.1.0",
    "morgan": "1.10.0",
    "compression": "1.7.0",
    "express-rate-limit": "7.1.0",
    "express-validator": "7.0.0",
    "bcryptjs": "2.4.0",
    "jsonwebtoken": "9.0.0",
    "dotenv": "16.3.0",
    "@prisma/client": "5.5.0",
    "prisma": "5.5.0",
    "redis": "4.6.0",
    "ioredis": "5.3.0",
    "bull": "4.11.0",
    "socket.io": "4.7.0",
    "multer": "1.4.0",
    "sharp": "0.32.0",
    "pdf-parse": "1.1.0",
    "openai": "4.14.0",
    "langchain": "0.0.180",
    "@langchain/openai": "0.0.14",
    "pinecone-client": "1.1.0",
    "replicate": "0.22.0",
    "aws-sdk": "2.1480.0",
    "nodemailer": "6.9.0",
    "winston": "3.11.0",
    "joi": "17.11.0",
    "uuid": "9.0.0",
    "moment": "2.29.0",
    "lodash": "4.17.0",
    "axios": "1.5.0"
  },
  "devDependencies": {
    "@types/express": "4.17.0",
    "@types/cors": "2.8.0",
    "@types/morgan": "1.9.0",
    "@types/compression": "1.7.0",
    "@types/bcryptjs": "2.4.0",
    "@types/jsonwebtoken": "9.0.0",
    "@types/multer": "1.4.0",
    "@types/node": "20.8.0",
    "@types/uuid": "9.0.0",
    "@types/lodash": "4.14.0",
    "@types/jest": "29.5.0",
    "typescript": "5.2.0",
    "nodemon": "3.0.0",
    "ts-node": "10.9.0",
    "jest": "29.7.0",
    "supertest": "6.3.0",
    "@types/supertest": "2.0.0",
    "eslint": "8.51.0",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
